{
  "dataDir": ".cache",
  "log4js": {
    "appenders": {
      "stdout": {
        "type": "stdout"
      },
      "server": {
        "type": "file",
        "filename": "./app.log"
      }
    },
    "categories": {
      "default": {
        "appenders": [
          "stdout",
          "server"
        ],
        "level": "info"
      }
    }
  }
}
